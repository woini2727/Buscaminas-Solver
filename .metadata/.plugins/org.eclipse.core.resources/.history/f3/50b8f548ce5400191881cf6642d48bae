package buscaminas_solver;

public class Celda {
	int value;
	int prob;
	boolean vista;
	public Celda(int value){
		this.value=value;
		this.vista=false;
	}
	
	public String vista_to_str() {
		
		String s1=Boolean.toString(this.vista);
		return s1;
	}
	public int getProb() {
		return prob;
	}

	public void setProb(int prob) {
		this.prob = prob; 
	}

	

	public boolean isVista() {
		return vista;
	}

	public void setVista(Celda[][] tablero, boolean vista) {
		this.vista = vista;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}
	
}
