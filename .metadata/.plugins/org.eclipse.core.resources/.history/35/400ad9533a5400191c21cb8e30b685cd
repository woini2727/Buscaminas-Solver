package buscaminas_solver;

public class Celda {
	int value;
	int prob;
	boolean vista;
	
	public int getProb() {
		return prob;
	}

	public void setProb(int prob) {
		this.prob = prob; 
	}

	public Celda(int value){
		this.value=value;
		this.vista=false;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}
	
}
